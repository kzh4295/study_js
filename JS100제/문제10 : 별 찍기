function printTriangle(height) {
  for (let i = 0; i < height; i++) {
    let row = '';
    for (let j = 0; j < height - i - 1; j++) {
      row += ' ';
    }
    for (let k = 0; k < 2 * i + 1; k++) {
      row += '*';
    }
    console.log(row);
  }
}

const height = 5; // 삼각형의 높이 설정
printTriangle(height);
